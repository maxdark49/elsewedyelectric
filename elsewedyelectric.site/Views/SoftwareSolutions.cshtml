@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.SoftwareSolutions>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}
<section id="top-page-image">
	<br />
<div class="row" >
	<div class="col-lg-12 col-md-12" style="text-align:center">
		<h2 style="text-align:center">@Model.TblHeader</h2>
	</div>
</div>
<br />
<div class="row" >
	<div class="col-lg-12 col-md-12" style="text-align:right">
		<strong>@Model.Description</strong>
	</div>
</div>
<div class="row" >
	<div class="col-lg-12 col-md-12" style="text-align:center">
		<strong>@Model.Strings.ToList()[0]</strong>
	</div>
</div>
<br />
<div class="row">
	<div class="col-lg-2 col-md-2">
	</div>
	<div class="col-lg-3 col-md-3">
		<a href="~/Views/DigitalGovernmentSolutionsG2GG2BG2C.cshtml"> @Model.Strings.ToList()[1] </a>
	</div>
	<div class="col-lg-3 col-md-3">
		<a href="~/Views/GISGeographicalInformationSystem.cshtml"> @Model.Strings.ToList()[2] </a>
	</div>
	<div class="col-lg-3 col-md-3">
		<a href="~/Views/CustomSoftwareApplication.cshtml"> @Model.Strings.ToList()[3]</a>
	</div>
</div>
<br />
<div class="row" >
	<div class="col-lg-12 col-md-12" style="text-align:center">
		<strong>@Model.Strings.ToList()[4]</strong>
	</div>
</div>
<br />
<div class="row">
	<div class="col-lg-2 col-md-2">
		<img width="100" height="100" src="@Model.Images.ToList()[0].Url()"/>	
	</div>
	<div class="col-lg-2 col-md-2">
		<img width="100" height="100" src="@Model.Images.ToList()[1].Url()"/>	
	</div>
	<div class="col-lg-2 col-md-2">
		<img width="100" height="100" src="@Model.Images.ToList()[2].Url()"/>	
	</div>
	<div class="col-lg-2 col-md-2">
		<img width="100" height="100" src="@Model.Images.ToList()[3].Url()"/>	
	</div>
	<div class="col-lg-2 col-md-2">
		<img width="100" height="100" src="@Model.Images.ToList()[4].Url()"/>	
	</div>
</div>

</section>



